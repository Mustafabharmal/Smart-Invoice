                                                        </h4>
                                                        <div class="productlinkset">
                                                            <h5>PT001</h5>
                                                        </div>
                                                        <div class="increment-decrement">
                                                            <div class="input-groups">
                                                                <a href="@Url.Action("ProductDecre", "POS", new { productId = productss.Details.product_id })">
                                                                    <input type="button" value="-" class="button-minus dec button">
                                                                </a>
                                                                <input type="text" name="child" value="@productss.Count" class="quantity-field">
                                                                <a href="@Url.Action("ProductDetails", "POS", new { productId = productss.Details.product_id })">
                                                                    <input type="button" value="+" class="button-plus inc button "></a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </li>
                                            <li>@productss.Details.price X @productss.Count = @(decimal.Parse(productss.Details.price) * productss.Count) Rs.</li>
                                            <li>
                                                <a class="confirm-text" href="@Url.Action("ProductDele", "POS", new { productId = productss.Details.product_id })">
                                                    <img src="~/assets/img/icons/delete-2.svg" alt="img">
                                                </a>
                                            </li>
                                        </ul>
                                    }
                                </div>
                            </div>
                            <div class="split-card">
                            </div>
                            <div class="card-body pt-0 pb-2">
                                <div class="setvalue">
                                    <ul>
                                        <li>
                                            <h5>Subtotal </h5>
                                            <h6>@ViewBag.Subtotal Rs.</h6>
                                        </li>
                                        <li>
                                            <h5>Tax 18% </h5>
                                            <h6>@(ViewBag.Tax) Rs.</h6>
                                        </li>
                                        <li class="total-value">
                                            <h5>Total </h5>
                                            <h6>@(ViewBag.Total) Rs.</h6>
                                        </li>
                                    </ul>
                                </div>
                                <div class="setvaluecash">
                                    <ul>
                                        <li>
                                            <a href="javascript:void(0);" class="paymentmethod">
                                                <img src="~/assets/img/icons/cash.svg" alt="img" class="me-2">
                                                Cash
                                            </a>
                                        </li>
                                        <li>
                                            <a href="javascript:void(0);" class="paymentmethod">
                                                <img src="~/assets/img/icons/debitcard.svg" alt="img" class="me-2">
                                                Debit
                                            </a>
                                        </li>
                                        <li>
                                            <a href="javascript:void(0);" class="paymentmethod">
                                                <img src="~/assets/img/icons/scan.svg" alt="img" class="me-2">
                                                Scan
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                                <a asp-area="" asp-controller="POS" asp-action="Innvoice">
                                    <div class="btn-totallabel">

                                        <h5>Checkout</h5>
                                        <h6>@(ViewBag.Total) Rs.</h6>

                                    </div>
