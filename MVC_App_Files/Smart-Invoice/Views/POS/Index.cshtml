                                </div>
                            </li>
                            @foreach (var category in ViewBag.Category)
                            {
                                <li id="@category.category_id">
                                    <div class="product-details">
                                        @*<img src="@category.ImageUrl" alt="img">*@
                                        @if (category.image != null && category.image.Length > 0)
                                        {
                                            var base64String = Convert.ToBase64String(category.image);

                                            <a href="javascript:void(0);" class="product-img">
                                                <img src="data:image/png;base64,@base64String" alt="Avatar" class="img-thumbnail" style="max-width: 50px; max-height: 50px;" />
                                            </a>
                                            @*<a href="javascript:void(0);">@user.first_name @user.last_name</a>*@
                                        }
                                        <h6>@category.product_name</h6>
                                    </div>
                                </li>
                            }
                        </ul>
                        <div class="tabs_container">
                            <div class="tab_content active" data-tab="All">
                                <div class="row ">
                                    @foreach (var product in ViewBag.Product)
                                    {
                                        <div class="col-lg-3 col-sm-6 d-flex">
                                            <div class="productset flex-fill">
                                                <div class="productsetimg">
                                                    @if (product.image != null && product.image.Length > 0)
                                                    {
                                                        var base64String = Convert.ToBase64String(product.image);

                                                        <a href="@Url.Action("ProductDetails", "POS", new { productId = product.product_id })" class="product-img">
                                                            <img src="data:image/png;base64,@base64String" alt="Avatar" class="img-thumbnail" />
                                                        </a>
                                                        @*<a href="javascript:void(0);">@user.first_name @user.last_name</a>*@
                                                    }
                                                    <h6>Qty: @product.quantity</h6>
                                                    <div class="check-product">

                                                        <i class="fa fa-check"></i>

                                                    </div>

                                                </div>
                                                <div class="productsetcontent">
                                                    <h5>@product.category_id</h5>
                                                    <h4>@product.product_name</h4>
                                                    <h6>@product.price</h6>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>

                            @foreach (var category in ViewBag.Category)
                            {
                                <div class="tab_content " data-tab="@category.category_id">
                                    <div class="row">
                                        @foreach (var producti in ViewBag.Product)
                                        {
                                            if (category.category_id == producti.category_id)
                                            {
                                                <div class="col-lg-3 col-sm-6 d-flex">
                                                    <div class="productset flex-fill">
                                                        <div class="productsetimg">
                                                            @if (producti.image != null && producti.image.Length > 0)
                                                            {
                                                                var base64String = Convert.ToBase64String(producti.image);

                                                                <a href="@Url.Action("ProductDetails", "POS", new { productId = producti.product_id })" class="product-img">
                                                                    <img src="data:image/png;base64,@base64String" alt="Avatar" class="img-thumbnail" />
                                                                </a>
                                                                @*<a href="javascript:void(0);">@user.first_name @user.last_name</a>*@
                                                            }
                                                            <h6>Qty: @producti.quantity</h6>
                                                            <div class="check-product">

                                                                <i class="fa fa-check"></i>

                                                            </div>
                                                        </div>
                                                        <div class="productsetcontent">
                                                            <h5>@category.product_name</h5>
                                                            <h4>@producti.product_name</h4>
                                                            <h6>@producti.price</h6>
                                                        </div>
                                                    </div>
                                                </div>}
                                        }
                                    </div>
                                </div>
                            }
                        </div>
                       
